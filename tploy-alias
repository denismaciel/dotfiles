 # Connection to server
 alias elk-burns='ssh -p 2124 -i ~/.ssh/tploy -N -f -o "ExitOnForwardFailure yes" -L 5612:0.0.0.0:5601 denis@workz-mitch-agent.westeurope.cloudapp.azure.com && open http://localhost:5612'
 alias es-burns='ssh -p 2124 -i ~/.ssh/tploy -N -f -o "ExitOnForwardFailure yes" -L 9210:0.0.0.0:9200 denis@workz-mitch-agent.westeurope.cloudapp.azure.com'
 alias ssh-mitch-agent1='ssh -p 2124 -i ~/.ssh/tploy denis@workz-mitch-agent.westeurope.cloudapp.azure.com'


